<launch>

  <arg name="manager"/>
  <arg name="spinnaker_camera_param_path" default="$(find spinnaker_camera_node)/param/bfs_ros1.yaml" />

  <node pkg="nodelet" type="nodelet" name="spinnaker_camera" args="load autoware/drivers/camera/spinnaker_camera_node $(arg manager)">
    <rosparam command="load" file="$(arg spinnaker_camera_param_path)" />
  </node>

</launch>
